<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Reservations.Page.MyDataPage"
             xmlns:popup="clr-namespace:Reservations.Page"
             xmlns:viewModel="clr-namespace:Reservations.modelViews"
             x:DataType="viewModel:MyDataViewModel"
             Title="MyDataPage"
             Style="{DynamicResource navBarBackgroundColorStyle}"
             Shell.TabBarIsVisible="False"
             Shell.NavBarIsVisible="False">
    
    <Grid RowDefinitions="*" ColumnDefinitions="*">
        <ActivityIndicator IsRunning="{Binding IsBusy}" IsVisible="{Binding IsBusy}" HorizontalOptions="Center" VerticalOptions="Center"/>
    </Grid>
</ContentPage>